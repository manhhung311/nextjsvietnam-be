{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateways/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateways/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateways",
  "projects": {
    "gateways": {
      "type": "application",
      "root": "apps/gateways",
      "entryFile": "main",
      "sourceRoot": "apps/gateways/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateways/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "mail": {
      "type": "library",
      "root": "libs/mail",
      "entryFile": "index",
      "sourceRoot": "libs/mail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mail/tsconfig.lib.json"
      }
    },
    "questions-and-answers": {
      "type": "application",
      "root": "apps/questions-and-answers",
      "entryFile": "main",
      "sourceRoot": "apps/questions-and-answers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/questions-and-answers/tsconfig.app.json"
      }
    }
  }
}